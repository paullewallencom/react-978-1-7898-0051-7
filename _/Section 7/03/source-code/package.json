{
  "name": "electron-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.3",
    "react-spinners": "^0.5.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "video.js": "^7.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "set ELECTRON_DISABLE_SECURITY_WARNINGS=true && set ELECTRON_START_URL=http://localhost:3000/ && electron .",
    "dev": "concurrently -k \"npm start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "prod-mac": "electron-packager . movie-app --platform=darwin --arch=x64"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.4",
    "electron-packager": "^13.1.0",
    "wait-on": "^3.2.0"
  }
}
